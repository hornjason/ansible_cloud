  seagull:
    metadata:
      extra: true
      hwprofile: null
      neName: NONE
      noa: false
      portmap:
        XMI: eth0
        XSI1: eth1
      role: NONE
      sgName: NONE
    properties:
      flavor:
        get_resource: oam_flavor
      image: MEATOS6.106.0
      key_name:
        get_param: sshkeypair
      name:

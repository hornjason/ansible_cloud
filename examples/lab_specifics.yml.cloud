        provider_physical_network: None
        provider_segmentation_id: None
        shared: False
        external: False
        router: internal
        subnets:
            name: xmi-subnet
            gateway_ip: 192.168.0.1
            allocation_pool_start: 192.168.0.2
            allocation_pool_end: 192.168.0.254

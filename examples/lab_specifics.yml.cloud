# CLOUD SPECIFICS
######################################

images:
  dsr_soam_img:
    source: nfs
    file_dir: /var/lib/openstack
    nfs_source_ip: 10.100.177.2
    nfs_source_dir: /var/exports/flavor
    container_format: bare
    disk_format: qcow2
  cloud_init_ol7_img:
    source: url
    file_dir: /var/lib/openstack
    url_source: http://10.240.121.66/images/myOL7image.qcow2
    container_format: bare
    disk_format: qcow2

flavors:
  dsr_soam_flavor:
    ram: 6000
    disk: 60
    ephemeral: 0
    vcpus: 4
    is_public: true
  m1_super_large_flavor:
    ram: 48000
    disk: 100
    ephemeral: 0
    vcpus: 32
    is_public: true

host_aggregates:

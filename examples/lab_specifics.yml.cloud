    policy: anti-affinity
  samehost:
    policy: affinity

users:
  - name: smog
    project: smog_project
    pass: changeme
    quotas:
    networks:
      - name: ext-net
        provider_network_type: flat
        provider_physical_network: physnet1        
        provider_segmentation_id: None
        shared: true
        external: true
        router: external
        subnets:
            name: ext-subnet
            gateway_ip: 10.75.183.1
            allocation_pool_start: 10.75.183.65
            allocation_pool_end: 10.75.183.126
            dns_nameservers:
             - 10.250.32.10
            enable_dhcp: False
            cidr: 10.75.183.0/25
      - name: xmi
